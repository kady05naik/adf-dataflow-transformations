{
	"name": "dataflow1",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "ds_employee",
						"type": "DatasetReference"
					},
					"name": "sourceemployee"
				},
				{
					"dataset": {
						"referenceName": "ds_department",
						"type": "DatasetReference"
					},
					"name": "sourcedepartment"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "ds_empdept",
						"type": "DatasetReference"
					},
					"name": "sinkjoinempdept"
				}
			],
			"transformations": [
				{
					"name": "joinempdept"
				}
			],
			"scriptLines": [
				"source(output(",
				"          empID as integer,",
				"          name as string,",
				"          country as string,",
				"          deptID as integer",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> sourceemployee",
				"source(output(",
				"          deptID as integer,",
				"          deptName as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> sourcedepartment",
				"sourceemployee, sourcedepartment join(sourceemployee@deptID == sourcedepartment@deptID,",
				"     joinType:'inner',",
				"     matchType:'exact',",
				"     ignoreSpaces: false,",
				"     broadcast: 'auto')~> joinempdept",
				"joinempdept sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     input(",
				"          customer_id as string,",
				"          customer_unique_id as string,",
				"          customer_zip_code_prefix as string,",
				"          customer_city as string,",
				"          customer_state as string",
				"     ),",
				"     umask: 0022,",
				"     preCommands: [],",
				"     postCommands: [],",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> sinkjoinempdept"
			]
		}
	}
}